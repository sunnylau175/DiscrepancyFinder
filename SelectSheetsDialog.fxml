<AnchorPane prefHeight="243.0" prefWidth="460.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="discrepencyfinder.SelectSheetsDialogController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Please select the worksheets" />
      <Label layoutX="14.0" layoutY="48.0" text="File 1" />
      <ListView fx:id="listFile1Sheet" layoutX="14.0" layoutY="67.0" prefHeight="109.0" prefWidth="200.0" />
      <Label layoutX="246.0" layoutY="48.0" text="File 2" />
      <ListView fx:id="listFile2Sheet" layoutX="246.0" layoutY="67.0" prefHeight="109.0" prefWidth="200.0" />
      <HBox alignment="CENTER" layoutX="-1.0" layoutY="193.0" prefHeight="34.0" prefWidth="460.0" spacing="12.0">
         <children>
            <Button fx:id="btnOK" mnemonicParsing="false" onAction="#handleOKAction" prefWidth="55.0" text="OK" />
            <Button fx:id="btnCancel" mnemonicParsing="false" onAction="#handleCanelAction" prefWidth="55.0" text="Cancel" />
         </children>
      </HBox>
   </children>
</AnchorPane>
